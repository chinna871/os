#include<stdio.h>
int main(){
 int m,n;
 printf("Enter rows:");
 scanf("%d",&m);
 printf("Enter columns:");
 scanf("%d",&n);
 int a[m][n],i,j;
 printf("Enter elements:\n");
 for(i=0;i<m;i++){
  for(j=0;j<n;j++){
   scanf("%d",&a[i][j]);
  }
 }
 printf("Matrix:\n");
 for(i=0;i<m;i++){
  for(j=0;j<n;j++){
   printf("%d\t",a[i][j]);
  }
  printf("\n");
 }
 printf("the 90 rotated matrix:");
for(i=0;i<n;i++){
 for(j=m-1;j>=0;j--){
  printf("%d\t",a[j][i]);
 }
 printf("\n");
}
}




#include<stdio.h>
int main(){
 int m,n;
 printf("Enter rows:");
 scanf("%d",&m);
 printf("Enter columns:");
 scanf("%d",&n);
 int a[m][n],i,j;
 printf("Enter elements:\n");
 for(i=0;i<m;i++){
  for(j=0;j<n;j++){
   scanf("%d",&a[i][j]);
  }
 }
 printf("Matrix:\n");
 for(i=0;i<m;i++){
  for(j=0;j<n;j++){
   printf("%d\t",a[i][j]);
  }
  printf("\n");
 }
 printf("the 90 anti clockwise rotated matrix:");
for(i=m-1;i>=0;i--){
 for(j=0;j<n;j++){
  printf("%d\t",a[j][i]);
 }
 printf("\n");
}
}



#include<stdio.h>
int main(){
 int m,n;
 printf("Enter rows:");
 scanf("%d",&m);
 printf("Enter columns:");
 scanf("%d",&n);
 int a[m][n],i,j;
 printf("Enter elements:\n");
 for(i=0;i<m;i++){
  for(j=0;j<n;j++){
   scanf("%d",&a[i][j]);
  }
 }
 printf("Matrix:\n");
 for(i=0;i<m;i++){
  for(j=0;j<n;j++){
   printf("%d\t",a[i][j]);
  }
  printf("\n");
 }
 printf("the 180 clockwise rotated matrix:\n");
for(i=m-1;i>=0;i--){
 for(j=n-1;j>=0;j--){
  printf("%d\t",a[i][j]);
 }
 printf("\n");
}
}















